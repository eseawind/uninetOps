package org.unism.util;

/**
 * 返回错误码信息
 * @author Wang_Yuliang
 */
public class Deco_errorCode {
	private String id;
	private String name;
	public Deco_errorCode(String id, String name){
		this.id = id;
		this.name = name;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
}
