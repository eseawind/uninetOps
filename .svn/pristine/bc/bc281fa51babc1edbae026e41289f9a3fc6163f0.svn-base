package org.unism.util;

/**
 * 设备大类信息
 * @author Wang_Yuliang
 */
public class Dev_btype {
	
	private String id;
	private String name;
	
	public Dev_btype(String id, String name){
		this.id = id;
		this.name = name;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
}
