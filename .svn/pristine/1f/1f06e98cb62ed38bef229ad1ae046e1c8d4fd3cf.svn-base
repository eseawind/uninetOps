package org.unism.util;

/**
 * 表5.	智能设备状态历史表 连接方式
 * @author Wang_Yuliang
 */
public class Dest_linkSts {

	private String id;
	private String name;
	
	public Dest_linkSts(String id, String name){
		this.id = id;
		this.name = name;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	
}
